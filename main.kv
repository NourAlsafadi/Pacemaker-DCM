<Connection>:
    text: 'Device Connection'
    orientation: 'vertical'
    padding: 150, 50, 150, 50
    
    Label:
        font_size: 20
        text: '   Connected User: \n\n '
        bold: True
        orientation: 'horizontal'
        padding: 0,0
        
    Label:
        text: root.status()
        font_size: 30
        padding: 0,0
        orientation: 'horizontal'
        color: root.color() 
	
	Button:
        text: 'Pacing Modes'
        font_size: 18
        opacity: 1 if root.showPacingModes() else 0
        on_press: root.pacingModesScreen()
		
    Button:
        text: root.disconnect()
        background_color: (0,0.7,1,1) if root.showPacingModes() else (0.0, 1.0, 0.0, 1.0)
        halign: 'center'
        font_size: 18

	Button:
		text: 'Logout'
        font_size: 18
		on_press: root.Logout()

<statusBar>:
    id: status

    Button:
		text: ' CONNECTION SCREEN '
		background_color: (0.75,0,0,1)
		on_press: root.returnConnection()
    
    Label:
		text: 'Current Mode:\n' + app.currentMode
        color: (1,0,0.1,0.8)
		
	Label:
		text: 'Status:\n CONNECTED'
		color: (0,1,0.5,1)
    
    Label:
        id: set
		text: 'Mode & Parameters: \nNOT SET' if not app.SET else 'Mode & Parameters: \nSET'
        active: False
		color: (0.94,0.78,0.04,1)
	
	Button:
        id: TB
		text: 'SET ALL'
		background_color: (0,0.75,0.25,1)
		on_press: root.setAll()
        on_press: app.SET = True
        
        
    
<title>:
    Label:
        orientation:'horizontal'
        text: 'PARAMETER'
        bold: True
        font_size: 20
    
    Label:
        orientation:'horizontal'
        text: 'PARAMETER VALUES'
        bold: True
        font_size: 20