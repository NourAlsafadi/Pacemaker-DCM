#:import p Parameters
<Parameters>:
    do_default_tab: False
    

    TabbedPanelItem:
        id: AOO
        text: 'AOO'
        on_press: app.currentMode = 'AOO'

        TableAOO:
            padding: 50, 50, 50, 50
            orientation: 'vertical'
            

    
    TabbedPanelItem:
        id: VOO
        text: 'VOO'
        on_press: app.currentMode = 'VOO'

        TableVOO:
            padding: 50, 50, 50, 50
            orientation: 'vertical'

    TabbedPanelItem:
        id: AAI
        text: 'AAI'
        on_press: app.currentMode = 'AAI'

        TableAAI:
            padding: 50, 50, 50, 50
            orientation: 'vertical'

    TabbedPanelItem:
        id: VVI
        text: 'VVI'
        on_press: app.currentMode = 'VVI'

        TableVVI:
            padding: 50, 50, 50, 50
            orientation: 'vertical'
    
    TabbedPanelItem:
        id: DOO
        text: 'DOO'
        on_press: app.currentMode = 'DOO'

        TableDOO:
            padding: 50, 50, 50, 50
            orientation: 'vertical'

		
		

