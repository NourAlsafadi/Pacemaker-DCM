<Connection>:
    text: 'Device Connection'
    orientation: 'vertical'
    padding: 150, 50, 150, 50
    
    Label:
        font_size: 20
        text: '   Connected Device: \n\n <Device Identifier here>'
        bold: True
        orientation: 'horizontal'
        padding: 0,0
        
    Label:
        text: root.status()
        font_size: 30
        padding: 0,0
        orientation: 'horizontal'
        color: root.color() 
	
	Button:
        text: 'Pacing Modes'
        font_size: 18
        opacity: 1 if root.showPacingModes() else 0
        on_press: root.pacingModesScreen()
		
    Button:
        text: root.disconnect()
        background_color: (0,0.7,1,1) if root.showPacingModes() else (0.0, 1.0, 0.0, 1.0)
        halign: 'center'
        font_size: 18

	Button:
		text: 'Logout'
        font_size: 18
		on_press: root.Logout()

<statusBar>:
    padding: 0

    Button:
		text: ' CONNECTION SCREEN '
		background_color: (0.75,0,0,1)
		on_press: root.returnConnection()

	# Label:
    #     padding: 5,0
	# 	text:' Connected Device:\n<Device Identifier here>'
    
    Label:
		text: 'Current Mode:\n' + app.currentMode
        color: (1,0,0.1,0.8)
		
	Label:
		text: 'Status:\n Connected'
		color: (0,1,0.5,1)
	
	Button:
		text: 'SET ALL'
		background_color: (0,0.75,0.25,1)
		on_press: root.setAll()
    
<title>:
    Label:
        orientation:'horizontal'
        text: 'PARAMETER'
        bold: True
        font_size: 20
    
    Label:
        orientation:'horizontal'
        text: 'PARAMETER VALUES'
        bold: True
        font_size: 20