#:import p Parameters
#:import d DatabaseManipulation
<Parameters>:
    do_default_tab: False
    

    TabbedPanelItem:
        id: AOO
        text: 'AOO'
        on_press: app.currentMode = 'AOO'
        on_press: d.currentM = 'AOO'

        TableAOO:
            padding: 50, 50, 50, 50
            orientation: 'vertical'
            

    
    TabbedPanelItem:
        id: VOO
        text: 'VOO'
        on_press: app.currentMode = 'VOO'
