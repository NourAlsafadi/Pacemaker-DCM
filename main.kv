#:import p Parameters
#:import d DatabaseManipulation
<Parameters>:
    id: parameters
    do_default_tab: False
    

    TabbedPanelItem:
        id: AOO
        text: 'AOO'
        on_press: app.currentMode = 'AOO'
        on_press: d.currentM = 'AOO'
        on_press: app.SET = False

        TableAOO:
            id: tAOO
            padding: 40, 0, 40, 0
            orientation: 'vertical'
            
            Label:
                text: 'Rate Adaptability'
                orientation: 'horizontal'

            CheckBox:
                id: cb 
                active: False
                on_press: 
                    if self.active: app.currentMode = 'AOOR' 
                    else: app.currentMode = 'AOO'
                on_press: 
                    if self.active: d.currentM = 'AOOR' 
                    else: d.currentM = 'AOO'  
                on_press: app.SET = False
                                   
            

    
    TabbedPanelItem:
        id: VOO
        text: 'VOO'
        on_press: app.currentMode = 'VOO'
        on_press: d.currentM = 'VOO'
        on_press: app.SET = False

        TableVOO:
            padding: 40, 0, 40, 0
            orientation: 'vertical'

            Label:
                text: 'Rate Adaptability'
                orientation: 'vertical'

            CheckBox:
                active: False
                orientation:'vertical'
                on_press: 
                    if self.active: app.currentMode = 'VOOR' 
                    else: app.currentMode = 'VOO'
                on_press: 
                    if self.active: d.currentM = 'VOOR' 
                    else: d.currentM = 'VOO'  
                on_press: app.SET = False

    TabbedPanelItem:
        id: AAI
        text: 'AAI'
        on_press: app.currentMode = 'AAI'
        on_press: d.currentM = 'AAI'
        on_press: app.SET = False

        TableAAI:
            padding: 40, 0, 40, 0
            orientation: 'vertical'

            Label:
                text: 'Rate Adaptability'
                orientation: 'horizontal'

            CheckBox:
                active: False
                on_press: 
                    if self.active: app.currentMode = 'AAIR' 
                    else: app.currentMode = 'AAI'
                on_press: 
                    if self.active: d.currentM = 'AAIR' 
                    else: d.currentM = 'AAI' 
                on_press: app.SET = False

    TabbedPanelItem:
        id: VVI
        text: 'VVI'
        on_press: app.currentMode = 'VVI'
        on_press: d.currentM = 'VVI'
        on_press: app.SET = False

        TableVVI:
            padding: 40, 0, 40, 0
            orientation: 'vertical'

            Label:
                text: 'Rate Adaptability'
                orientation: 'horizontal'

            CheckBox:
                active: False
                on_press: 
                    if self.active: app.currentMode = 'VVIR' 
                    else: app.currentMode = 'VVI'
                on_press: 
                    if self.active: d.currentM = 'VVIR' 
                    else: d.currentM = 'VVI' 
                on_press: app.SET = False
    
    TabbedPanelItem:
        id: DOO
        text: 'DOO'
        on_press: app.currentMode = 'DOO'
        on_press: d.currentM = 'DOO'
        on_press: app.SET = False

        TableDOO:
            padding: 40, 0, 40, 0
            orientation: 'vertical'

            Label:
                text: 'Rate Adaptability'
                orientation: 'horizontal'

            CheckBox:
                active: False
                on_press: 
                    if self.active: app.currentMode = 'DOOR' 
                    else: app.currentMode = 'DOO'
                on_press: 
                    if self.active: d.currentM = 'DOOR' 
                    else: d.currentM = 'DOO' 
                on_press: app.SET = False

		
		

